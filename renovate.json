{
  "extends": [
    "github>preparingforexams/renovate-config"
  ],
  "regexManagers": [
    {
      "datasourceTemplate": "pypi",
      "fileMatch": [
        "^\\.github/workflows/.+\\.ya?ml$"
      ],
      "matchStrings": [
        "pip install (?<depName>[_a-z]+==(?<currentValue>\\d+\\.\\d+\\.\\d+)"
      ]
    }
  ]
}
